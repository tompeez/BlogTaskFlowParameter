<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="region-edit-create-btf">
    <default-activity>region</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>mode</name>
      <value>#{pageFlowScope.mode}</value>
      <class>java.lang.String</class>
      <required/>
    </input-parameter-definition>
    <view id="region">
      <page>/region.jsff</page>
    </view>
    <method-call id="Commit">
      <method>#{bindings.Commit.execute}</method>
      <outcome>
        <fixed-outcome>back</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Rollback">
      <method>#{bindings.Rollback.execute}</method>
      <outcome>
        <fixed-outcome>back</fixed-outcome>
      </outcome>
    </method-call>
    <parent-action id="parentAction1">
      <parent-outcome>back</parent-outcome>
    </parent-action>
    <control-flow-rule id="__5">
      <from-activity-id>region</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>commit</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__7">
        <from-outcome>rollback</from-outcome>
        <to-activity-id>Rollback</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>Commit</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>back</from-outcome>
        <to-activity-id>parentAction1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>Rollback</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>back</from-outcome>
        <to-activity-id>parentAction1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
